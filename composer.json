{
  "name": "solodkiy/doctrine-array-hydrator",
  "description": " An array to entity hydrator for Doctrine 2 entities ",
  "keywords": ["php", "doctrine", "hydrator", "array"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexey Solodkiy",
      "email": "alexey@solodkiy.by"
    },
    {
      "name": "pmill",
      "email": "dev.pmill@gmail.com"
    }
  ],
  "require": {
      "php": "^7.1 || ^8.1",
      "doctrine/orm": "^2.5 || ^3.2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "psr-4": {"Solodkiy\\Doctrine\\Hydrator\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {
      "Solodkiy\\Doctrine\\Hydrator\\Test\\": "tests/unit/",
      "Solodkiy\\Doctrine\\Hydrator\\Test\\Fixture\\": "tests/fixtures/"
    }
  }
}
